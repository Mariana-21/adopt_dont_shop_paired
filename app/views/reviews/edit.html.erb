<%= form_tag "/reviews/#{@review.id}", method: :patch do %>
<%= label_tag "Title" %>
<%= text_field_tag :title, @review.title %><br/>

<%= label_tag "Rating" %>
<%= text_field_tag :rating, @review.rating %><br/>

<%= label_tag "Content" %><br/>
<%= text_area_tag 'content', @review.content =%><br/>

<%= label_tag "Age" %>
<%= text_field_tag :picture, @review.picture %><br/>

<%= submit_tag "Update Review" %>
<% end %>
